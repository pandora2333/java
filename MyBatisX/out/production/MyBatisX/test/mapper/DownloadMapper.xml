<MyMapper namespace="pers.pandora.test.mapper.DownloadMapper">
	<!--可以使用lt,gt,le,ge表示大小关系，配合返回值为List使用-->
	<!--如gt #{id} and deptno lt #{no}-->
	<!--注意:mapper中定义参数若为实体类，则#{no}对应参数实体类中属性名,否则随意取名-->
	<select id="queryForOne" resultType="pers.pandora.test.Download">
		select * from download where id = #{id}
  	</select>
	<insert id="insert">
		insert into download value(null,#{fileName},#{path},#{time},#{upper})
	</insert>
	<delete id="deleteById">
		delete from download where id in (#{id1})
	</delete>
	<update id="update">
		update download set fileName=#{fileName},path=#{path},time=#{time},upper=#{upper} where id = #{id}
	</update>
</MyMapper>